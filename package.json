{
  "name": "rolldown",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "babel": "babel es-src --ignore '*.client.js,client/*.js' -d dist",
    "watch-tsc": "tsc -w --preserveWatchOutput",
    "watch-roll": "rollup -c -w",
    "watch-babel": "babel es-src --watch --skip-initial-build --ignore '*.client.js,client/*.js' -d dist",
    "watch-build": "concurrently -c bgBlue,bgGreen,bgYellow npm:watch-tsc npm:watch-roll npm:watch-babel",
    "watch-server": "nodemon --watch dist --exec 'node --inspect dist/server' ",
    "watch": "npm run build; concurrently -c bgBlue,bgGreen,bgYellow,white npm:watch-tsc npm:watch-roll npm:watch-babel npm:watch-server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc && rollup -c && npm run babel",
    "clean": "rm -rf resources/ es-src/ dist/ && echo 'its clean again'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/callsite": "^1.0.30",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/jquery": "^3.3.4",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "callsite": "^1.0.0",
    "class-validator": "^0.9.1",
    "connect-flash": "^0.1.1",
    "cookie-session": "^2.0.0-beta.3",
    "csstips": "^0.3.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-async-errors": "^3.0.0",
    "fast-glob": "^2.2.2",
    "html-formatter": "^0.1.9",
    "install": "^0.12.1",
    "jquery": "^3.3.1",
    "multer": "^1.3.1",
    "npm": "^6.3.0",
    "reflect-metadata": "^0.1.12",
    "request-local": "^1.0.5",
    "sanitize-html": "^1.18.2",
    "sqlite3": "^4.0.2",
    "touch": "^3.1.0",
    "typed-html": "^0.6.0",
    "typeorm": "^0.2.7",
    "typestyle": "^2.0.1"
  },
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "concurrently": "^3.6.1",
    "rollup-plugin-node-resolve": "^3.3.0"
  }
}
